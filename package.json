{
  "name": "ngx-echarts-demo",
  "version": "6.0.1",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:lib && ng serve",
    "build": "npm run build:lib && ng build",
    "build:lib": "ng build ngx-echarts --prod && npm run copy:readme",
    "changelog:init": "conventional-changelog -i CHANGELOG.md -s -p angular -r 0",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "copy:readme": "ncp ./README.md ./dist/ngx-echarts/README.md",
    "publish": "npm publish ./dist/ngx-echarts --access=public",
    "publish:demo": "ng build --base-href /ngx-echarts/ --prod && gh-pages -d ./dist/ngx-echarts-demo",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "conventional-changelog-cli": "^2.0.11",
    "core-js": "^2.5.4",
    "echarts": "^4.2.0-rc.2",
    "ncp": "^2.0.0",
    "ngx-bootstrap": "^3.1.1",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular-devkit/build-ng-packagr": "^0.10.4",
    "@angular/cli": "~6.1.4",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.4.0",
    "protractor": "~5.4.0",
    "raw-loader": "^0.5.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
